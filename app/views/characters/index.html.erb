<h1>Characters#index</h1>
<p>Find me in app/views/characters/index.html.erb</p>

<h1>List of all Characters</h1>
<ol>
<% @characters.each do |character| %>
    <li>
      <%= link_to character.name, character_path(character) %>
      alias:
      <%= link_to character.alias, character_path(character) %>
      superpower:
      <%= link_to character.superpower.name, character_path(character) %>
    </li>
<% end %>
</ol>
<br>
<%= link_to "New Character", new_character_path %>